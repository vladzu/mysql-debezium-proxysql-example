
admin_variables=
{
    admin_credentials="proxysql-admin:proxysql-admin"
    mysql_ifaces="0.0.0.0:6032"
}

mysql_variables=
{
    threads=1
    max_connections=2000
    default_query_delay=0
    default_query_timeout=180000
    have_compress=true
    poll_timeout=2000
    interfaces="0.0.0.0:3306"
    default_schema="information_schema"
    stacksize=1048576
    connect_timeout_server=10000
    monitor_username="proxysql-monitor"
    monitor_password="proxysql-monitor"
    monitor_history=600000
    monitor_connect_interval=20000
    monitor_ping_interval=10000
    monitor_read_only_interval=1500
    monitor_read_only_timeout=500
    ping_interval_server_msec=10000
    ping_timeout_server=500
    commands_stats=true
    sessions_sort=true
    connect_retries_on_failure=10
    monitor_galera_healthcheck_timeout=1500
    set_query_lock_on_hostgroup=0
    enable_client_deprecate_eof=0
    enable_server_deprecate_eof=0
}

mysql_servers=({
hostname="mysql1"
port=3306
hostgroup=1
},{
hostname="mysql2"
port=3306
hostgroup=2
},{
hostname="mysql1"
port=3306
hostgroup=2
}
)

mysql_users=({
active=1
username="debezium"
password="dbz"
default_hostgroup=1
transaction_persistent=1
fast_forward=1
})

mysql_replication_hostgroups=({
writer_hostgroup=1
reader_hostgroup=2
check_type="read_only"
})